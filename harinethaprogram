
#include <stdio.h>

int main(){
     char str[100];
     
     printf("Enter String");
     scanf("%s",&str);
     
    printf("%s\n",str);
    int l = strlen(str);
    char t= str[l/2];
    int m = l/2;
    for(int i=0;i<strlen(str);i++){
         for(int j=l;j<=strlen(str);j++){
             printf("%c ",str[i]);
        
    }
    l--;
      printf("\n");
    }
    

    return 0;
}
